{
  "name": "mashcontrol",
  "version": "1.0.0",
  "description": "mash controller",
  "main": "mashControl.js",
  "scripts": {
    "publish": "git tag -a v$npm_package_version -m 'tag version v$npm_package_version'",
    "postpublish": "git push --tags",
    "pretest": "jshint *.js app/**/*.js",
    "test": "istanbul cover _mocha test --recursive",
    "prestart": "git pull && jshint",
    "start": "node mashControl.js",
    "predocker": "docker build -t ozsie/mash-control:$npm_package_version .",
    "docker": "docker run -d --name mash-control ozsie/mash-control:$npm_package_version",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "start-offline": "node mashControl.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ozsie/mashControl.git"
  },
  "keywords": [
    "mash",
    "beer",
    "control",
    "automated"
  ],
  "author": "Oscar Djupfeldt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ozsie/mashControl/issues"
  },
  "homepage": "https://github.com/Ozsie/mashControl#readme",
  "dependencies": {
    "connect": "^3.5.0",
    "express": "^4.14.0",
    "body-parser": "~1.15",
    "angular": "~1.5",
    "angular-resource": "~1.5",
    "n3-charts": "^2.0.28",
    "d3": "^3.5.17",
    "jshint": "^2.9.4",
    "winston": "^2.3.1",
    "mc-gpio": "^1.0.3",
    "mc-tempsensor": "^1.0.4",
    "grpc": "^1.0.0",
    "bootstrap": "3.3.7",
    "jquery": "3.2.0",
    "push.js": "^0.0.12",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "mocha": "^3.2.0",
    "chai": "^4.0.0-canary.1",
    "chai-http": "^3.0.0",
    "istanbul": "^0.4.5",
    "coveralls": "^2.13.1",
    "gpio-mock": "^1.1.0"
  }
}
