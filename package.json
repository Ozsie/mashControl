{
  "name": "mashcontrol",
  "version": "1.0.0",
  "description": "mash controller",
  "main": "mashControl.js",
  "scripts": {
    "publish": "git tag -a v$npm_package_version -m 'tag version v$npm_package_version'",
    "postpublish": "git push --tags",
    "pretest": "jshint *.js app/**/*.js",
    "test": "mocha test --recursive",
    "prestart": "jshint",
    "start": "node mashControl.js",
    "predocker": "docker build -t ozsie/mash-control:$npm_package_version .",
    "docker": "docker run -d --name mash-control ozsie/mash-control:$npm_package_version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ozsie/mashControl.git"
  },
  "keywords": [
    "mash",
    "beer",
    "control",
    "automated"
  ],
  "author": "Oscar Djupfeldt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ozsie/mashControl/issues"
  },
  "homepage": "https://github.com/Ozsie/mashControl#readme",
  "dependencies": {
    "connect": "^3.5.0",
    "express": "^4.14.0",
    "body-parser": "~1.15",
    "angular": "~1.5",
    "angular-resource": "~1.5",
    "n3-charts": "^2.0.28",
    "d3": "^3.5.17",
    "jshint": "^2.9.4",
    "winston": "^2.3.1",
    "mocha": "^3.2.0",
    "chai": "^4.0.0-canary.1",
    "chai-http": "^3.0.0",
    "mc-gpio": "^1.0.3",
    "mc-tempsensor": "^1.0.4",
    "grpc": "^1.0.0",
    "bootstrap": "3.3.7",
    "jquery": "3.2.0"
  }
}
